{%import (
	"github.com/PietroCarrara/aenianos/internal/context"
)%}

{% func Login(ctx context.Context) %}

{%= Header(ctx) %}

<form method="post">
	<input type="text" name="usuario">
	<input type="password" name="senha">
	<input type="submit" value="Login">
</form>

{%= Footer(ctx) %}

{% endfunc %}
